{
  "name": "pvansi-design-system-example",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "web"
  ],
  "license": "MIT",
  "author": "Pedro Ivan Salas PeÃ±a",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petergtam/pvansi-design-system-example.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "format": "prettier --write .",
    "prepare": "husky",
    "build": "bob build",
    "lint": "eslint .",
    "preversion": "kacl prerelease",
    "version": "npm run build && kacl release && git add -A -f lib",
    "postversion": "git push && git push --tags && npm publish"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@pvansi/kacl": "bitbucket:pvansi/kacl",
    "@react-native/eslint-config": "^0.83.0",
    "@react-native/typescript-config": "^0.82.1",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-native": "^0.82.1",
    "react-native-builder-bob": "^0.40.16",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.71",
    "react-native-web": ">=0.19"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  }
}
